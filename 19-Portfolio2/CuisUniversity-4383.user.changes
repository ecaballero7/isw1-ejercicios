!AccountSummaryTest methodsFor: 'tests - ReceptiveAccount' stamp: 'G. B. 11/15/2020 15:24:48' prior: 50939789!
test02AccountWhitOneDepositTransactionsReturnsCorrectlyReport

	| anAccount expectedReport |
	
	anAccount := ReceptiveAccount new.
	Deposit register: 100 on: anAccount.
	expectedReport := OrderedCollection new.
	expectedReport  add: 'DepÛsito por 100'.
	expectedReport  add: 'Balance = 100'.
	
	self assert: expectedReport equals: anAccount summaryReport. ! !
!ReceptiveAccount methodsFor: 'accessing' stamp: 'G. B. 11/15/2020 15:25:24' prior: 50940771!
summaryReport
	|report|
	report := OrderedCollection new.
	transactions do: [:aTransaction | 		report add: (aTransaction affectReport)].
	report add: 'Balance = ' , (self balance) asString . 
	^ report. ! !
!Portfolio methodsFor: 'accessing' stamp: 'G. B. 11/15/2020 15:25:36' prior: 50940670!
summaryReport
	
	|report|
	
	report := OrderedCollection new.
