!methodRemoval: Deposit #affectSummaryReport: stamp: 'G. B. 11/15/2020 16:56:55'!
affectSummaryReport: anAccountSummary
	^ anAccountSummary fromDeposit: value.
	
!
!AccountSummary methodsFor: 'as yet unclassified' stamp: 'G. B. 11/15/2020 16:57:04' prior: 50942662!
accountSummaryReportFor: anAccount
	|report|
	
	report := OrderedCollection new.
	(anAccount transactions) do: [:aTransaction | report add: (aTransaction affectReport: self)].
	report add: 'Balance = ' , (anAccount balance) asString . 
	^ report.! !

!methodRemoval: Deposit #affectTransferNet: stamp: 'G. B. 11/15/2020 16:57:15'!
affectTransferNet: aTransferNet
	^ aTransferNet fromDeposit: value.

!
!TransferNet methodsFor: 'as yet unclassified' stamp: 'G. B. 11/15/2020 16:57:20' prior: 50944144!
calculateTransferNetFor: anAccount

	currentTransferNet := 0. 
	(anAccount transactions) do: [:transaction | transaction affectReport: self ].
	
	^ currentTransferNet 
	
	! !
